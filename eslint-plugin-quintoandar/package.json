  },
  "keywords": [
    "eslint plugin",
    "custom rules",
    "eslint"
  ],
  "author": "Quinto Andar",
  "license": "MIT",
  "homepage": "https://github.com/quintoandar/eslint-config-quintoandar/tree/master/eslint-plugin-quintoandar#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1"
  }
}
