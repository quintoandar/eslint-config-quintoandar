{
  "name": "eslint-plugin-quintoandar",
  "version": "1.3.0",
  "description": "An eslint-plugin for PWA-Tenants custom rules",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./reports/coverage/lcov.info | coveralls",
