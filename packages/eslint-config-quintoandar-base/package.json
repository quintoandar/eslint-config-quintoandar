{
  "name": "@quintoandar/eslint-config-quintoandar-base",
  "author": "Quinto Andar",
  "version": "0.0.1",
  "description": "ESLint Shareable Config for QuintoAndar's Base JS projects",
  "license": "ISC",
  "homepage": "https://github.com/quintoandar/eslint-config-quintoandar/tree/master/eslint-config-quintoandar-base#readme",
  "main": "index.js",
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "index.js",
    "README.md",
    "CHANGELOG.md",
    "rules"
  ],
  "keywords": [
    "eslint",
    "sharable-config",
    "quintoandar",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quintoandar/eslint-config-quintoandar.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "peerDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "5.9.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "^2.14.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.4.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testRegex": "tests/.*\\.test\\.js"
  }
}
